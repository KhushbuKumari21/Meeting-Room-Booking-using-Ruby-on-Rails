<h1>Edit Meeting Room</h1>

<%= form_with model: @meeting_room, url: admin_meeting_room_path(@meeting_room), local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :capacity %>
    <%= form.number_field :capacity %>
  </div>

  <div>
    <%= form.submit "Update Meeting Room" %>
  </div>
<% end %>

<%= link_to "Back", admin_meeting_rooms_path, class: "button" %>
